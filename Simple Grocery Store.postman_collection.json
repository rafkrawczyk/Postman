      "name": "API Status",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "id": "d79faeec-a4d7-486f-a56c-487d6308ac50",
            "exec": [
              "console.clear();",
              "",
              "const person = {",
              "    'first name': \"John\",",
              "    age: 30,",
              "    city: \"New York\",",
              "    sayHello: () => 'Hello, my first name is ' + person['first name']",
              "};",
              "",
              "person['e-mail'] = 'john@example.com'",
              "",
              "let json = JSON.stringify(person);",
              "let personNew = JSON.parse(json);",
              "",
              "console.log(person.sayHello());",
              "console.log(person['e-mail']);",
              "console.log(json);",
              "console.log(personNew);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "id": "4b86e3d5-441d-4fa6-bb59-a35c20751151",
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "",
              "pm.test(\"Status = UP\", function () {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData.status).to.eql(\"UP\");",
              "});",
              "});",
              "",
              "const response = pm.response.json();",
              "",
              "pm.test(\"Status is UP\", () => {",
              "    pm.expect(response.status).to.eql(\"UP\");",
              "});",
              "",
              "console.log(pm.response.json());"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
